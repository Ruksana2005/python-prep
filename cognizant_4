def mismatch(n,s):
    k=0
    for i in range(len(s)//2):
        if s[i]!=s[-(i+1)]:
            k+=1
    return k
print(mismatch(5,"11101"))
print(mismatch(6,"111001"))

output
1
2

import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int i,k=0;
      int n=sc.nextInt();
      String s=sc.next();
      for(i=0;i<(int)n/2;i++){
        if(s.charAt(i)!=s.charAt((n-i)-1)){
          k++;
        }
      }
      System.out.println(k);
  }
}


