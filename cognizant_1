
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      String s = scan.nextLine();
      System.out.println(s);
      int t=0;
      int d=0,rem;
      for(int i=0;i<s.length();i++){
          t=t+(int)s.charAt(i)-48; //sum of all digits
          rem=(int)s.charAt(i)-48+d*10;
          d=rem%11;
      }
      if(d==0){
          System.out.println("Divisible by 11");
      }
      else
      {
          System.out.println("Not Divisible by 11");
          System.out.println("Remainder : "+d);
          
      }
      System.out.println(t);
  }
}

output
121
121
Divisible by 11
4


111
111
Not Divisible by 11
Remainder : 1
3

import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int n = scan.nextInt();
      int[] p = new int[n];
      
      for(int i =0; i < n; i++)
      p[i] = scan.nextInt();
      
      // jagged array 
      int[][] moons = new int[n][];
      for(int i =0; i < n; i++)
        moons[i] = new int[p[i]];
      
      for(int i =0; i < n; i++){
        for(int j =0; j <p[i]; j++){
          moons[i][j]=scan.nextInt();
        }
      }
      int m=0,r=0,s=0;  
      for(int i =0; i < n; i++){
        for(int j =0; j <p[i]; j++){
          System.out.printf("%3d", moons[i][j]);
          
          if(moons[i][j]>m){
            m=moons[i][j];
            r=i;
            s=j;
            
          }
          
          
        }
        System.out.println();
        
      }
      int maxsum=0,row=-1;
      for(int i=0;i<n;i++){
        int sum=0;
        for(int j=0;j<p[i];j++){
          sum+=moons[i][j];
        }
        if(maxsum<sum){
          maxsum=sum;
          row=i;
        }
      }
      System.out.println("Maximum sum row : "+row);
      System.out.println(m);
      System.out.println("Row : "+r);
      System.out.println("Column : "+s);
      
      
  }
}


output
4
4 5 3 2
3 4 5 6
3 5 7 4 2 
3 9 8
5 10


STDIN
