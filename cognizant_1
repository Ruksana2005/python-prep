-- create
  CREATE TABLE emp (
   emp_id int, 
   name varchar(20), 
   mail_ref varchar(10), 
   primary key(emp_id, mail_ref)
  );

insert into emp values(1, "aruna", "uni_001");
insert into emp values(1, "aruna", "uni_002");
insert into emp values(2, "arun", "gov_001");
insert into emp values(2, "arun", "gov_002");
insert into emp values(2, "arun", "gov_003");
insert into emp values(3, "aru", "ugc_002");

select emp_id, name,
       count(*) as mails
from emp
group by emp_id, name
order by mails desc
limit 2;

Output:
+--------+-------+-------+
| emp_id | name  | mails |
+--------+-------+-------+
|      2 | arun  |     3 |
|      1 | aruna |     2 |
+--------+-------+-------+









