[100, 7, 200, 9, 300, 3, 400, 4, 500, 6]



##between inlcudes starting and ending values and <> is used for denoting not equal to 
create table patients(
patient_id int primary key,
name varchar(50) not null,
age int,
gender char(5) not null,
diagnosis varchar(50)
);
insert into patients values(1,"Alice",30,'F','Flu');
insert into patients values(2,"Bob",45,'M','Diabetes');
insert into patients values(3,"Carol",29,'F','Asthma');
insert into patients values(4,"Dan",50,'M','HyperTension');
insert into patients values(5,"Eve",35,'F','Migranene');
insert into patients values(6,"Frank",40,'M','Flu');
insert into patients values(7,"Grace",60,'F','Diabetes');
insert into patients values(8,"Henry",22,'M','Flu');
insert into patients values(9,"Irene",55,'F','Migranene');
insert into patients values(10,"Jack",65,'M','HyperTension');
select * from patients;
select diagnosis,round(avg(age),2) as AGE,count(*) as No_of_patients from patients group by diagnosis order by No_of_patients desc;
alter table patients add Age_Group varchar(50);

update patients set Age_Group="Young" where age<=30 and age>=1;
update patients set Age_Group="Adult" where age>=31 and age<=45;
update patients set Age_Group="Middle Aged" where age>=46 and age<=59;
update patients set Age_Group="Senior" where age>=60;
select * from patients;
SELECT 
    patient_id,
    name,
    age,
    gender,
    diagnosis,
    CASE 
        WHEN age <= 30 THEN 'young'
        WHEN age > 30 AND age < 45 THEN 'adult'
        WHEN age >= 45 AND age < 60 THEN 'middle aged'
        WHEN age >= 60 THEN 'senior'
    END AS Age_group_new
FROM patients;

select * from patients;
select * from patients where diagnosis='Flu' or diagnosis='Asthma';
select patient_id,name,age,gender from patients where diagnosis <> 'flu' and diagnosis <> 'Asthma';
select * from patients where diagnosis in ('Flu','Asthma');

select * from patients where diagnosis not in ('Flu','Asthma');
select name,diagnosis from patients where diagnosis!='Flu';
select diagnosis,count(*) as No_of_patients from patients group by diagnosis having No_of_patients>1 order by No_of_patients desc;
select * from patients where age between 40 and 50;
select * from patients where name like 'F%' OR name like 'D%';

select * from patients where name like 'a%';

