  3  5  7  4  2
  3  9  8
  5 10
Maximum sum row : 1
10
Row : 3
Column : 1


//xor 
//no.of subsets - (i+1)*(n-i)
//i=Index
//n=size of array
//arr=[1,2,3]
//1-3,2-4,3-3
// in xor if the no.of occurance is even then it will be equal to 0 else the same number
//2 -- 0
//1 -- 1
//3 -- 3
//1 xor 3 -- 2(ans)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int n = scan.nextInt();
      int[] arr = new int[n];
      for(int i =0; i < n; i++) arr[i] = scan.nextInt();
      System.out.println(Arrays.toString(arr));
      int res = 0; 
      for(int i =0; i < n; i++){
        int freq = (i+1) *(n-i);
        if(freq %2 != 0) res = res ^ arr[i];
      }
      
      System.out.println(res);
  }
}

3
1 2 3
STDIN
Output:

[1, 2, 3]
2


import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int n1 = scan.nextInt();
      scan.nextLine();
      String s1 = scan.nextLine();
      int n2 = scan.nextInt();
      scan.nextLine();
      String s2 = scan.nextLine();
      StringBuilder sb = new StringBuilder(); 
      int i =0, j = 0;
      while( i < n1 && j < n2){
        sb.append(s1.charAt(i));
        i++; 
        sb.append(s2.charAt(j));
        j++;
      }
      while(i < n1) {
        sb.append(s1.charAt(i));
        i++;
      }
      while(j < n2) {
        sb.append(s2.charAt(j));
        j++;
      }
      String res = sb.toString();
      System.out.println(res);
    }
}


output
5
aruna
8
bhargavi
STDIN
Output:

abrhuanragavi


##replace the array elements with its maximum element at the right,replace the last element with -1
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int n = scan.nextInt();
      int[] arr = new int[n];
      for(int i =0; i < n; i++) arr[i] = scan.nextInt();
      fun(arr);
      System.out.println(Arrays.toString(arr));
   }
   static void fun(int[] arr){
     int right = arr.length - 1; 
     int max = -1; 
     while(right >= 0){
       if(arr[right] > max){
         int temp = max; 
         max = arr[right];
         arr[right] = temp;
         right--;
         continue;
      }
      arr[right] = max;
      right--;
     }
     return;
   }
}

output
6
100 9 8 6 7 5

import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int n = scan.nextInt();
      int[] arr = new int[2*n];
      for(int i =0; i < 2*n; i++) arr[i] = scan.nextInt();
      System.out.println(Arrays.toString(arr));
      ArrayList<Integer> list = fun(arr, n);
      System.out.println(list); 
  }
  static ArrayList<Integer> fun(int[] arr, int n){
    ArrayList<Integer> list = new ArrayList<Integer>(); 
    for(int i =0; i < n; i++){
      list.add(arr[i]);
      list.add(arr[n+i]);
    }
    return list;
  }
}

output
5
100 200 300 400 500 7 9 3 4 6
STDIN
Output:

[100, 200, 300, 400, 500, 7, 9, 3, 4, 6]
[100, 7, 200, 9, 300, 3, 400, 4, 500, 6]
STDIN
Output:

[9, 8, 7, 7, 5, -1]

##combine the 2 arrays into single
import java.util.*;
public class Main{
  public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int[] order= new int[n];
    int[] size=new int[n];
    for(int i=0;i<n;i++){
      order[i]=sc.nextInt();
    }
    for(int i=0;i<n;i++){
      size[i]=sc.nextInt();
    }
    int[] arr=new int[2*n];
    int i=0,j=0,k=0;
    while(k<2*n){
      arr[k]=order[i];
      k++;
      i++;
      arr[k]=size[j];
      j++;
      k++;
    }
    
      System.out.println(Arrays.toString(arr));
 
  }
}

output
 
5
100 200 300 400 500
1 2 3 4 5

STDIN
Output:

[100, 1, 200, 2, 300, 3, 400, 4, 500, 5]

import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int n = scan.nextInt();
      int[] arr = new int[2*n];
      for(int i =0; i < 2*n; i++) arr[i] = scan.nextInt();
      System.out.println(Arrays.toString(arr));
      ArrayList<Integer> list = fun(arr, n);
      System.out.println(list); 
  }
  static ArrayList<Integer> fun(int[] arr, int n){
    ArrayList<Integer> list = new ArrayList<Integer>(); 
    for(int i =0; i < n; i++){
      list.add(arr[i]);
      list.add(arr[n+i]);
    }
    return list;
  }
}

output
